<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="snoob.gdd.mapper.LogMapper">
  <resultMap id="BaseResultMap" type="snoob.gdd.model.Log">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="id" jdbcType="VARCHAR" property="id" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="req_url" jdbcType="VARCHAR" property="reqUrl" />
    <result column="req_method" jdbcType="VARCHAR" property="reqMethod" />
    <result column="req_args" jdbcType="VARCHAR" property="reqArgs" />
    <result column="controller_method" jdbcType="VARCHAR" property="controllerMethod" />
    <result column="msg" jdbcType="VARCHAR" property="msg" />
  </resultMap>
  <!-- 批量删除 -->
  <delete id="customDelete" parameterType="java.util.List">
    delete from log where id in
    <foreach close=")" collection="list" index="index" item="id" open="(" separator=",">
      #{id}
    </foreach>
  </delete>
</mapper>